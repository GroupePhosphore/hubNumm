                                "schema": {
                                    "description": "Statistiques sans ID Datalake",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Success"
                                        },
                                        "data": {
                                            "allOf": [
                                                {
                                                    "$ref": "#/components/schemas/StatisticWithoutIDClientRivalis"
                                                },
